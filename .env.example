# SUHA FPS+ v3.0 Configuration
# Copy this file to .env and configure your settings

# Discord Bot Configuration
DISCORD_BOT_TOKEN="your_discord_bot_token_here"
DISCORD_NOTIFICATION_CHANNEL_ID="your_channel_id_here"

# Database Configuration (Optional)
DATABASE_URL="postgresql://USER:PASSWORD@localhost:5432/db"

# Web Dashboard Configuration
WEB_DASHBOARD_HOST="localhost"
WEB_DASHBOARD_PORT=5000
WEB_DASHBOARD_SECRET_KEY="change-me-to-a-random-secret"

# AI System Configuration
AI_LEARNING_ENABLED=true
AI_MONITORING_INTERVAL=30
AI_CONFIDENCE_THRESHOLD=0.7

# Performance Thresholds
CPU_WARNING_THRESHOLD=75
CPU_CRITICAL_THRESHOLD=90
MEMORY_WARNING_THRESHOLD=80
MEMORY_CRITICAL_THRESHOLD=95
TEMPERATURE_WARNING_THRESHOLD=75
TEMPERATURE_CRITICAL_THRESHOLD=85

# Notification Settings
NOTIFICATION_LEVEL="medium"  # low, medium, high
ENABLE_DISCORD_NOTIFICATIONS=true
ENABLE_CONSOLE_NOTIFICATIONS=true
ENABLE_FILE_NOTIFICATIONS=true

# Auto-optimization Settings
AUTO_OPTIMIZATION_ENABLED=true
AUTO_FPS_OPTIMIZATION=true
AUTO_MEMORY_CLEANUP=true

# Logging Configuration
LOG_LEVEL="INFO"  # DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_TO_FILE=true
LOG_TO_CONSOLE=true

# OAuth Configuration (Optional - for advanced features)
NEXTAUTH_SECRET="change-me"
GITHUB_ID="github-client-id"
GITHUB_SECRET="github-client-secret"
GOOGLE_CLIENT_ID="google-client-id"
GOOGLE_CLIENT_SECRET="google-client-secret"

# Advanced Settings
ENABLE_PREDICTIVE_ANALYSIS=true
ENABLE_SMART_NOTIFICATIONS=true
RATE_LIMIT_NOTIFICATIONS=true
QUIET_HOURS_START=22  # 10 PM
QUIET_HOURS_END=8     # 8 AM
